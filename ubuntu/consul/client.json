{
  "server": false,
  "datacenter": "{{datacenter}}",
  "data_dir": "/var/lib/consul",
  "ui_dir": "/usr/share/consul/ui",
  "encrypt": "{{key}}",
  "log_level": "INFO",
  "enable_syslog": true,
  "leave_on_terminate": true,
  "rejoin_after_leave": true,
  "start_join": [{{cluster}}],
  "retry_join": [{{cluster}}]
}
